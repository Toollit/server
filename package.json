{
  "name": "toollit-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=production npx tsc -p .",
    "start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/src/app.js",
    "typeorm:prod": "NODE_ENV=production typeorm-ts-node-commonjs schema:sync -d ./dist/src/config/data-source",
    "migration:prod:create": "NODE_ENV=production typeorm-ts-node-commonjs migration:create ./dist/src/migrations/migration",
    "migration:prod:generate": "NODE_ENV=production typeorm-ts-node-commonjs migration:generate ./dist/src/migrations/migration -d ./dist/src/config/data-source",
    "migration:prod:run": "NODE_ENV=production typeorm-ts-node-commonjs migration:run -d ./dist/src/config/data-source && npm run migration:dev:move",
    "migration:prod:revert": "NODE_ENV=production typeorm-ts-node-commonjs migration:revert -d ./dist/src/config/data-source",
    "migration:prod:show": "NODE_ENV=production typeorm-ts-node-commonjs migration:show -d ./dist/src/config/data-source",
    "migration:prod:move": "mv src/migrations/*.ts src/migrations/backup",
    "dev": "NODE_ENV=development nodemon",
    "typeorm:dev": "NODE_ENV=development typeorm-ts-node-commonjs schema:sync -d src/config/data-source",
    "migration:dev:create": "NODE_ENV=development typeorm-ts-node-commonjs migration:create src/migrations/migration",
    "migration:dev:generate": "NODE_ENV=development typeorm-ts-node-commonjs migration:generate src/migrations/migration -d src/config/data-source",
    "migration:dev:run": "NODE_ENV=development typeorm-ts-node-commonjs migration:run -d src/config/data-source && npm run migration:dev:move",
    "migration:dev:revert": "NODE_ENV=development typeorm-ts-node-commonjs migration:revert -d src/config/data-source",
    "migration:dev:show": "NODE_ENV=development typeorm-ts-node-commonjs migration:show -d src/config/data-source",
    "migration:dev:move": "mv src/migrations/*.ts src/migrations/backup"
  },
  "author": "seungwon.code@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.299.0",
    "@aws-sdk/client-ssm": "^3.533.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.9.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.6.7",
    "redis-cli": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.12",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/hpp": "^0.2.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemon": "^1.19.6",
    "@types/passport": "^1.0.12",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "npm": "10.2.4",
    "node": "20.11.1"
  }
}
