{
  "name": "getit-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node dist/app.js",
    "dev": "npm run typeorm && cross-env NODE_ENV=development nodemon",
    "build": "rm -rf dist && npx tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs schema:sync -d src/data-source",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/migration",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source && npm run move-migrations",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source",
    "move-migrations": "mv src/migrations/*.ts src/migrations/backup"
  },
  "author": "seungwon.code@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.299.0",
    "@types/nodemon": "^1.19.2",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql": "^2.14.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.1",
    "nodemon": "^2.0.20",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.5.2"
  }
}
